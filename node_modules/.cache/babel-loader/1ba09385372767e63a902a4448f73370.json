ink :to={name: \"Login\"}><button class=\"button is-danger\">Sair</button></router-link> \r\n    \r\n\r\n\r\n           \r\n\r\n            <table class=\"table center is-fullwidth\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>Id</th>\r\n                        <th>Nome</th>\r\n                        <th>E-mail</th>\r\n                        <th>Imagem</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"user in users\" :key=\"user.id\">\r\n                        <td>{{user.id}}</td>\r\n                        <td>{{user.first_name}}</td>\r\n                        <td>{{user.email}}</td>\r\n                        <td><img :src=\"user.avatar\" alt=\"\"></td>\r\n                        <td><router-link :to=\"{name: 'Edit'}\"><button class=\"button is-success\">Editar</button></router-link></td>    \r\n                        <td><button class=\"button is-danger\" @click=\"showModalUser(user.id)\">Deletar</button></td> \r\n                    </tr>\r\n\r\n                </tbody>\r\n\r\n            </table>\r\n\r\n                    <div :class=\"{modal: true, 'is-active': showModal}\">\r\n                <div class=\"modal-background\"></div>\r\n                <div class=\"modal-content\">\r\n                   <div class=\"card\">\r\n                       <header class=\"card-header\">\r\n                           <div class=\"card-header-title\">\r\n                               VocÃª quer realmente deletar?\r\n                           </div>\r\n                       </header>\r\n                       <div class=\"card-content\">\r\n                           <div class=\"content\">\r\n                               <p>Deletar mesmo assim</p>\r\n                           </div>\r\n                       </div>\r\n                       <footer class=\"card-footer\">\r\n                           <a href=\"#\" class=\"card-footer-item\" @click=\"hideModal()\">Cancelar</a>\r\n                            <a href=\"#\" class=\"card-footer-item\" @click=\"deleteUser()\">Deletar</a>\r\n                       </footer>\r\n                   </div>\r\n                </div>\r\n                <button class=\"modal-close is-large\" aria-label=\"close\" @click=\"hideModal()\"></button>\r\n                </div>\r\n\r\n\r\n\r\n               </div>\r\n          \r\n\r\n  \r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\"\r\n\r\nexport default {\r\n\r\n        created(){\r\n            axios.get(\"https://reqres.in/api/users?page=1\").then(res => {\r\n                this.users = res.data.data\r\n            }).catch(err => {\r\n                console.log(err)\r\n            })\r\n        },\r\n\r\n        methods: {\r\n            hideModal(){\r\n                this.showModal = false\r\n            },\r\n            showModalUser(id){\r\n                this.deleteUserId = id\r\n                this.showModal = true\r\n            },\r\n            deleteUser(){\r\n                    axios.delete(\"https://reqres.in/api/users?page=1\" + this.deleteUserId).then(res => {\r\n                        console.log(res)\r\n                        this.showmodal = false\r\n                    })\r\n            }\r\n        },\r\n\r\n    data(){\r\n\r\n        return {\r\n            users: {\r\n\r\n            },\r\n            showModal: false,\r\n            deleteUserId: -1\r\n        }\r\n\r\n\r\n\r\n    }\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"sourceRoot":"src/views"}]}